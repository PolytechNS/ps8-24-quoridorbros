<?xml version='1.0' encoding='utf-8'?>
<widget id="com.cordova.quoribros" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:android="http://schemas.android.com/apk/res/android">
    <name>Quoribros</name>
    <description>Sample Apache Cordova App</description>
    <author email="dev@cordova.apache.org" href="https://cordova.apache.org">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />

     <preference name="hostname" value="quoribros.ps8.academy"/>
    <preference name="scheme" value="http"/>
    <access origin="http://quoribros.ps8.academy" />
    <uses-permission android:name="android.permission.INTERNET"/>
    <edit-config
            xmlns:android="http://schemas.android.com/apk/res/android"
            file="app/src/main/AndroidManifest.xml"
            mode="merge"
            target="/manifest/application">
        <application android:usesCleartextTraffic="true" />
    </edit-config>
    <icon src="res/icon.png"/>
</widget>
